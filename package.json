{
  "name": "@joniras/ngx-editor2",
  "version": "5.0.0",
  "description": "WYSIWYG Editor for Angular Applications",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "docs": "compodoc && npm run publish:docs",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "postbuild": "npm run build:package",
    "prebuild": "npm run lint",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prerelease": "npm run build",
    "publish:docs": "node ./scripts/publishdocs.js",
    "release": "ng-packagr -p package.json && npm publish dist --access=public",
    "start": "ng serve --port 4201",
    "serve:docs": "compodoc --serve --open --output=\"docs/\"",
    "test": "ng test",
    "test:single-run": "ng test --watch false"
  },
  "keywords": [
    "angular-editor",
    "angular-wysiwyg-editor",
    "wysiwyg-editor",
    "ngx-editor",
    "ngx-editor2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joniras/ngx-editor2.git"
  },
  "homepage": "https://github.com/Joniras/ngx-editor2",
  "peerDependencies": {
    "@angular/common": ">=8.2.14",
    "@angular/core": ">=8.2.14",
    "@angular/forms": ">=8.2.14",
    "font-awesome": ">=4.7.0",
    "ngx-bootstrap": ">=5.2.0",
    "rxjs": ">=6.5.3",
    "@fortawesome/angular-fontawesome": ">=0.5.0",
    "@fortawesome/fontawesome-svg-core": ">=1.2.25",
    "@fortawesome/free-brands-svg-icons": ">=5.11.2",
    "@fortawesome/free-regular-svg-icons": ">=5.11.2",
    "@fortawesome/free-solid-svg-icons": ">=5.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.19",
    "@angular/animations": "^8.2.14",
    "@angular/cli": "^8.3.19",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.15",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.4.6",
