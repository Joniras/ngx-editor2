    "font-awesome": "^4.7.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "inquirer": "^7.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.7.1",
    "ngx-bootstrap": "^5.2.0",
